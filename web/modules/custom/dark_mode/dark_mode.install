<?php

/**
 * @file
 * Install, update and uninstall functions for the Dark Mode module.
 */

/**
 * Implements hook_install().
 */
function dark_mode_install() {
  \Drupal::messenger()->addStatus(__FUNCTION__);
}

/**
 * Implements hook_uninstall().
 */
function dark_mode_uninstall() {
  \Drupal::messenger()->addStatus(__FUNCTION__);
}
